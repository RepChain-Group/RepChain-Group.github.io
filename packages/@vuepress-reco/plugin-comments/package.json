{
  "name": "@vuepress-reco/vuepress-plugin-comments",
  "version": "2.0.0-beta.2",
  "description": "Comments plugin for vuepress-theme-reco or other vuepress theme.",
  "keywords": [
    "vuepress-theme-reco",
    "comment",
    "vuepress"
  ],
  "author": "reco_luan <recoluan@outlook.com>",
  "homepage": "https://vuepress-theme-reco.recoluan.com",
  "license": "MIT",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-reco/vuepress-theme-reco-next.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build -w tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.{d.ts,vue,scss,css,svg}\" lib"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.33",
    "@vuepress/core": "2.0.0-beta.33",
    "@vuepress/plugin-theme-data": "2.0.0-beta.33",
    "@vuepress/utils": "2.0.0-beta.33",
    "valine": "1.4.14"
  },
  "bugs": {
    "url": "https://github.com/vuepress-reco/vuepress-theme-reco-next/issues"
  }
}
